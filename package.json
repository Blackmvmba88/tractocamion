{
  "name": "tractocamion-web",
  "version": "4.0.0",
  "description": "Sistema Integral de Gestión Logística - Cross-platform Web Application",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "node src/server/index.js",
    "monitor": "node src/scripts/process-monitor.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:create": "sequelize-cli db:create"
  },
  "keywords": [
    "logistics",
    "tractocamion",
    "fleet-management",
    "cross-platform"
  ],
  "author": "Tractocamión Team",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "pg": "^8.17.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.5"
  }
}
